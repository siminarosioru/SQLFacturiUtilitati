CREATE DATABASE [Utilitati]
 CONTAINMENT = NONE
 ON  PRIMARY 
( NAME = N'Utilitati', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL15.MSSQLSERVER2020\MSSQL\DATA\Utilitati.mdf' , SIZE = 8192KB , MAXSIZE = UNLIMITED, FILEGROWTH = 65536KB )
 LOG ON 
( NAME = N'Utilitati_log', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL15.MSSQLSERVER2020\MSSQL\DATA\Utilitati_log.ldf' , SIZE = 8192KB , MAXSIZE = 2048GB , FILEGROWTH = 65536KB )
 WITH CATALOG_COLLATION = DATABASE_DEFAULT
GO

IF (1 = FULLTEXTSERVICEPROPERTY('IsFullTextInstalled'))
begin
EXEC [Utilitati].[dbo].[sp_fulltext_database] @action = 'enable'
end
GO

